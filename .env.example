# Optional: WebSocket server URL for remote control
# Example: WORKSTATION_WS_URL=ws://localhost:9000/ws
#WORKSTATION_WS_URL=

# Optional: HTTP control server port (default: 8787)
#HTTP_CONTROL_PORT=8787

# Optional: Start in full-screen mode (default: false, uses maximized)
#FULLSCREEN=true

# User Type: 'provider' or 'ledcarewall'
# This determines which states and URLs are available
USER_TYPE=ledcarewall

# Auto-start on system boot (default: true)
# Set to 'false' to disable automatic startup
AUTO_START=true

# State URLs - LED CareWall Display (when USER_TYPE=ledcarewall)
# Screensaver: Exact URL (no params appended)
SCREENSAVER_URL=splash

# Carescape view URL (query params like roomId will be appended)
CARESCAPE_URL=https://fe-app.oneroomhealth.app/ledwallview/care

# In-session CareWall view URL (params appended)
IN_SESSION_URL=https://fe-app.oneroomhealth.app/ledwallview/ma

# Goodbye/Thank you screen URL (params appended)
GOODBYE_URL=https://fe-app.oneroomhealth.app/ledwallview/endAppt

# State URLs - Provider Workstation (when USER_TYPE=provider)
# Provider screensaver - exact URL (no params appended)
PROVIDER_SCREENSAVER_URL=https://fe-app.oneroomhealth.app/wall/provider-display/screensaver

# Provider in-session view - params like roomId, inviteId will be appended
PROVIDER_IN_SESSION_URL=https://fe-app.oneroomhealth.app/extensionproviderview